{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 127.0.0.1",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "clear_bucket": "aws s3 rm s3://app.rapidsell.io --recursive",
    "sync_bucket": "aws s3 sync ./dist s3://app.rapidsell.io",
    "deploy": "npm run build && npm run clear_bucket && npm run sync_bucket",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run dev",
    "lint": "eslint --ext .vue,.ts src --fix",
    "verify": "npm run lint"
  },
  "dependencies": {
    "@capacitor/app": "5.0.6",
    "@capacitor/camera": "^5.0.6",
    "@capacitor/cli": "5.2.2",
    "@capacitor/core": "5.2.2",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.2.2",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@heroicons/vue": "^2.0.18",
    "@ionic/vue": "^7.2.1",
    "@ionic/vue-router": "^7.2.1",
    "axios": "^1.4.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "sass": "^1.64.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
